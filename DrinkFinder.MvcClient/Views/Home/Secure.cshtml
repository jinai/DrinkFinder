@{
    ViewData["Title"] = "Secure Page, don't leak !";
}
<h1>@ViewData["Title"]</h1>
<h2>Claims</h2>

<dl>
    @foreach (var claim in Model.Principal.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in Model.Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>